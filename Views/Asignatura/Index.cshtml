@model Asignatura 

@{
    ViewData["Title"] = "Informacion Asignatura";
    Layout = "Simple"; 
}

<h1>@ViewData["Title"] </h1>

<strong>Desde la vista normal</strong>
<p>
    Nombre: @Model.Nombre 
    <br/>
    Id: @Model.Id
</p>

<strong>Desde la vista parcial</strong>
<partial name ="ObjetoEscuelaSimple"/>    <!--NOS PERMITE INVOCAR VISTAS DESDE EL CONTROLADOR
                                            Se le puede pasar otro parámetro -> model="" 
                                            y pasarle otro modelo como parámetro, sin embargo,
                                            en este caso asume que es el mismo modelo 
                                            del cual tiene este archivo (Asignatura) -->

<p><i> @ViewBag.CosaDinamica </i></p> 
<p><i> @ViewBag.Fecha </i></p> 